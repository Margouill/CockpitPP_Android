                            android:id="@+id/ka50_overhead_nav_sw_center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_white_btn_up"
                            android:scaleType="fitXY"
                            android:visibility="invisible" />

                        <ImageButton
                            android:id="@+id/ka50_overhead_nav_sw_down"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_white_btn_down"
                            android:contentDescription="@null"
                            android:visibility="visible"
                            tools:scaleType="fitXY" />

                        <Button
                            android:id="@+id/ka50_overhead_nav_sw"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null" />

                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.795"
                    android:orientation="vertical"
                    tools:weightSum="2">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.5"
                        android:orientation="horizontal"
                        android:weightSum="6">

                        <View
                            android:id="@+id/view"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.43" />

                        <ImageView
                            android:id="@+id/ka50_pui800_ready_led1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="8dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_green_led_on"
                            android:visibility="visible" />

                        <View
                            android:id="@+id/view2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.68" />

                        <ImageView
                            android:id="@+id/ka50_pui800_ready_led2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="3dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_green_led_on"
                            android:visibility="visible" />

                        <View
                            android:id="@+id/view3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.87" />

                        <ImageView
                            android:id="@+id/ka50_pui800_ready_led3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="3dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_green_led_on"
                            android:visibility="visible" />

                        <View
                            android:id="@+id/view4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.68" />

                        <ImageView
                            android:id="@+id/ka50_pui800_ready_led4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="8dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_green_led_on" />

