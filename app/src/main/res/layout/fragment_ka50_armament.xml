                        android:visibility="visible" />

                    <ImageButton
                        android:id="@+id/ka50_pui800_burst_sw_down"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ka50_white_btn_down"
                        android:contentDescription="@null"
                        android:visibility="invisible"
                        tools:scaleType="fitXY" />

                    <ImageButton
                        android:id="@+id/ka50_pui800_burst_sw_mid"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ka50_white_btn_mid"
                        android:contentDescription="@null"
                        android:visibility="invisible"
                        tools:scaleType="fitXY" />

                    <Button
                        android:id="@+id/ka50_pui800_burst_sw"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null" />
                </RelativeLayout>

                <View
                    android:id="@+id/view24"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.95" />

