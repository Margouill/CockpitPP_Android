                        android:layout_height="0dp"
                        android:layout_weight="0.5"
                        android:orientation="horizontal"
                        tools:weightSum="6">

                        <View
                            android:id="@+id/view6"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.43" />

                        <ImageView
                            android:id="@+id/ka50_pui800_present_led1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="8dp"
                            android:layout_weight="0.4"
                            android:src="@drawable/ka50_yellow_led_on" />

                        <View
                            android:id="@+id/view7"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.67" />

                        <ImageView
                            android:id="@+id/ka50_pui800_present_led2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="1dp"
                            android:layout_marginLeft="1dp"
                            android:layout_marginTop="7dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_yellow_led_on" />

                        <View
                            android:id="@+id/view8"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.875" />

                        <ImageView
                            android:id="@+id/ka50_pui800_present_led3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="7dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_yellow_led_on" />

                        <View
                            android:id="@+id/view9"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.69" />

                        <ImageView
                            android:id="@+id/ka50_pui800_present_led4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="8dp"
                            android:layout_weight="0.40"
                            android:src="@drawable/ka50_yellow_led_on" />

                        <View
