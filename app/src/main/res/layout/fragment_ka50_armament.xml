                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="-20sp"
                            android:layout_weight="1.75"
                            android:fontFamily="@font/ttf_digital_display_tfb"
                            android:gravity="center_horizontal|top"
                            android:text="88"
                            android:textAlignment="gravity"
                            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                            android:textColor="@color/m2kCGreen"
                            android:textSize="100sp"
                            android:textStyle="bold"
                            tools:typeface="monospace" />

                        <View
                            android:id="@+id/view12"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.37" />

                        <TextView
                            android:id="@+id/ka50_pui800_weaponremain_textview"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="-20sp"
                            android:layout_weight="1.75"
                            android:fontFamily="@font/ttf_digital_display_tfb"
                            android:gravity="center_horizontal|top"
                            android:text="88"
                            android:textAlignment="gravity"
                            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                            android:textColor="@color/m2kCGreen"
                            android:textSize="100sp"
                            android:textStyle="bold"
                            tools:typeface="monospace" />

                        <View
                            android:id="@+id/view13"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.62" />

                        <TextView
                            android:id="@+id/ka50_pui800_canonremain_textview"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="-20sp"
                            android:layout_weight="1.75"
                            android:fontFamily="@font/ttf_digital_display_tfb"
                            android:gravity="center_horizontal|top"
                            android:text="88"
                            android:textAlignment="gravity"
                            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                            android:textColor="@color/m2kCGreen"
                            android:textSize="100sp"
                            android:textStyle="bold"
                            tools:typeface="monospace" />

                        <View
                            android:id="@+id/view14"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.71" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.2"
                        android:orientation="horizontal">

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/view27"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                tools:weightSum="9">

                <View
                    android:id="@+id/view34"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.75" />

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.65">

                    <Button
                        android:id="@+id/ka50_pui800_aa_jettison"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="23dp"
                        android:background="@drawable/circle_button" />

                </RelativeLayout>

                <View
                    android:id="@+id/view30"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.85" />

                <RelativeLayout
