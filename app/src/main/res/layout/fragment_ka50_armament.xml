                    <ImageButton
                        android:id="@+id/ka50_pui800_round_sw_down"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ka50_white_btn_down"
                        android:contentDescription="@null"
                        android:visibility="invisible"
                        tools:scaleType="fitXY" />

                    <Button
                        android:id="@+id/ka50_pui800_round_sw"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null" />
                </RelativeLayout>

                <View
                    android:id="@+id/view25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.25" />

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <ImageButton
                        android:id="@+id/ka50_pui800_rate_sw_up"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ka50_white_btn_up"
                        android:scaleType="fitXY"
                        android:visibility="invisible" />

                    <ImageButton
                        android:id="@+id/ka50_pui800_rate_sw_down"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ka50_white_btn_down"
                        android:contentDescription="@null"
                        android:visibility="visible"
                        tools:scaleType="fitXY" />

                    <Button
                        android:id="@+id/ka50_pui800_rate_sw"
