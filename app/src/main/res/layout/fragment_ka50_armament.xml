                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/circle_button_discret" />

                </RelativeLayout>

                <View
                    android:id="@+id/view31"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.58" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    tools:weightSum="1">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <ImageButton
                            android:id="@+id/ka50_pui800_arm_jettison_sw_up"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_white_btn_up"
                            android:scaleType="fitXY"
                            android:visibility="invisible" />

                        <ImageButton
                            android:id="@+id/ka50_pui800_arm_jettison_sw_down"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_white_btn_down"
                            android:contentDescription="@null"
                            android:visibility="visible"
                            tools:scaleType="fitXY" />

                        <Button
                            android:id="@+id/ka50_pui800_arm_jettison_sw"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null" />

                    </RelativeLayout>

                </LinearLayout>

                <View
                    android:id="@+id/view32"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.7"
                    android:orientation="vertical"
                    tools:weightSum="1">

                    <View
                        android:id="@+id/view35"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.60" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.40">

                        <ImageButton
                            android:id="@+id/ka50_pui800_jettison_atgm_sw_up"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="3dp"
                            android:layout_marginLeft="3dp"
                            android:layout_marginEnd="3dp"
                            android:layout_marginRight="3dp"
                            android:background="@drawable/ka50_weapon_jettison_on"
                            android:scaleType="fitXY"
                            android:visibility="visible" />

                        <ImageButton
                            android:id="@+id/ka50_pui800_jettison_atgm_sw_down"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="3dp"
                            android:layout_marginLeft="3dp"
                            android:layout_marginEnd="3dp"
                            android:layout_marginRight="3dp"
                            android:background="@drawable/ka50_weapon_jettison_off"
                            android:contentDescription="@null"
                            android:visibility="invisible"
                            tools:scaleType="fitXY" />

                        <Button
                            android:id="@+id/ka50_pui800_jettison_atgm_sw"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null" />

                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:id="@+id/view33"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.87" />

            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/view26"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.8"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                tools:weightSum="10">

                <View
                    android:id="@+id/view20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.7" />

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <ImageButton
                        android:id="@+id/ka50_pui800_mode_sw_up"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ka50_white_btn_up"
