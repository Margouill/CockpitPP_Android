<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.astazou.cockpitplusplus.fragments.Ka50_UV26"
    tools:ignore="MissingPrefix">

    <ImageView
        android:id="@+id/layout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:adjustViewBounds="true"
        android:contentDescription="@null"
        android:scaleType="fitXY"
        android:src="@drawable/ka50_uv26_bkg" />

    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:gravity="center"
        android:orientation="vertical"
        android:weightSum="2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.84"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3.38"
                android:orientation="vertical"
                tools:weightSum="4">

                <View
                    android:id="@+id/view59"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.6" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.1"
                    android:orientation="horizontal"
                    tools:weightSum="8">

                    <View
                        android:id="@+id/view17"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1" />

                    <ImageView
                        android:id="@+id/ka50_uv26_led_left"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.9"
                        android:src="@drawable/ka50_red_led_on" />

                    <TextView
                        android:id="@+id/ka50_uv26_tv"
                        fontPath="fonts/digital_display_tfb.ttf"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4.3"
                        android:gravity="center_horizontal|top"

                        android:text="888"
                        android:textColor="@color/m2kCRed"
                        android:textSize="100sp" />

                    <ImageView
                        android:id="@+id/ka50_uv26_led_right"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.9"
                        android:src="@drawable/ka50_red_led_on" />

                    <View
                        android:id="@+id/view51"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.9" />
                </LinearLayout>


                <View
                    android:id="@+id/view60"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    tools:weightSum="4">

                    <View
                        android:id="@+id/view62"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.3" />

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.4">

                        <ImageButton
                            android:id="@+id/ka50_uv26_side_left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_btn_left"
                            android:scaleType="fitXY"
                            android:visibility="invisible" />

                        <ImageButton
                            android:id="@+id/ka50_uv26_side_center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_btn_center"
                            android:contentDescription="@null"
                            android:visibility="visible"
                            tools:scaleType="fitXY" />

                        <ImageButton
                            android:id="@+id/ka50_uv26_side_right"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_btn_right"
                            android:contentDescription="@null"
                            android:visibility="invisible"
                            tools:scaleType="fitXY" />

                        <Button
                            android:id="@+id/ka50_uv26_side_bt"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null" />
                    </RelativeLayout>

                    <View
                        android:id="@+id/view63"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.65" />

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.4">

                        <ImageButton
                            android:id="@+id/ka50_uv26_quant_left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_btn_left"
                            android:scaleType="fitXY"
                            android:visibility="visible" />

                        <ImageButton
                            android:id="@+id/ka50_uv26_quant_right"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ka50_btn_right"
                            android:contentDescription="@null"
                            android:visibility="invisible"
                            tools:scaleType="fitXY" />

                        <Button
                            android:id="@+id/ka50_uv26_quant_bt"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null" />
                    </RelativeLayout>

                    <View
                        android:id="@+id/view73"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.25" />

                </LinearLayout>

                <View
                    android:id="@+id/view61"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.27" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.16"
            android:orientation="horizontal"
            android:weightSum="5">

            <View
                android:id="@+id/view72"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.9" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.4"
                android:orientation="vertical"
                tools:weightSum="3.5">

                <Button
                    android:id="@+id/ka50_uv26_number_bt"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.95"
                    android:background="@null" />

                <Button
                    android:id="@+id/ka50_uv26_sequence_bt"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.90"

                    android:background="@null" />

                <Button
                    android:id="@+id/ka50_uv26_stop_bt"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.90"
                    android:background="@null" />

                <View
                    android:id="@+id/view70"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.76" />
            </LinearLayout>

            <View
                android:id="@+id/view19"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.6" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.4"
                android:orientation="vertical"
                tools:weightSum="3.5">

                <Button
                    android:id="@+id/ka50_uv26_interval_bt"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.95"
                    android:background="@null" />

                <Button
                    android:id="@+id/ka50_uv26_reset_bt"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.9"
                    android:background="@null" />

                <Button
                    android:id="@+id/ka50_uv26_start_bt"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.90"
                    android:background="@null" />

                <View
                    android:id="@+id/view71"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.76" />
            </LinearLayout>

            <View
                android:id="@+id/view49"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.69" />
        </LinearLayout>

    </LinearLayout>

</FrameLayout>
